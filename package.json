{
  "name": "paratii-portal",
  "version": "0.0.0",
  "description": "Paratii Portal",
  "main": "index.js",
  "repository": "https://github.com/Paratii-Video/paratii-portal.git",
  "author": "Benjamin Baum <benjaminjbaum@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev":
      "yarn run clean && yarn run prep && webpack-dev-server -d --content-base ./build --hot --progress --colors",
    "build":
      "yarn run clean && yarn run prep && NODE_ENV=production webpack --progress --colors",
    "prep": "mkdir build/ && cp src/index.html build/",
    "clean": "rm -rf build/",
    "lint": "eslint src test --cache",
    "test": "NODE_ENV=test karma start karma.conf.js",
    "test:dev": "yarn run test -- --single-run false",
    "quality": "yarn run lint && yarn run flow",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["prettier --write", "eslint --cache", "git add"]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-object-rest-spread": "^0.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.61.0",
    "husky": "^0.14.3",
    "karma": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.9.2",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "jasmine-core": "^2.8.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.3.3"
  }
}
